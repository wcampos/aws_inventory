[pylint]
ignore=
    migrations,
    manage.py,
    __init__.py,
    apps.py,
    admin.py,
    models.py,
    serializers.py,
    urls.py,
disable=
    import-error,
    missing-module-docstring,
    missing-function-docstring,
    missing-class-docstring,
    no-self-argument,
    no-member,
    undefined-loop-variable,
    no-value-for-parameter,
    invalid-name,
    pointless-string-statement,
    too-few-public-methods,
    too-many-locals
