<p1>VPC Data</p1>
<table>
  <!-- table header -->
  {% if vpc_results %}
  <tr>
     {% for key in vpc_results[0] %}
     <th> {{ key }} </th>
     {% endfor %}
  </tr>
  {% endif %}

  <!-- table rows -->
  {% for dict_item in vpc_results %}
  <tr>
     {% for value in dict_item.values() %}
     <td> {{ value }} </td>
     {% endfor %}
  </tr>
  {% endfor %}
</table>

<p1>Subnets Data</p1>
<table>
  <!-- table header -->
  {% if subnet_results %}
  <tr>
     {% for key in subnet_results[0] %}
     <th> {{ key }} </th>
     {% endfor %}
  </tr>
  {% endif %}

  <!-- table rows -->
  {% for dict_item in subnet_results %}
  <tr>
     {% for value in dict_item.values() %}
     <td> {{ value }} </td>
     {% endfor %}
  </tr>
  {% endfor %}
</table>

</body>
